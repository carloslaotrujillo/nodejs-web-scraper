{
	"name": "nodejs-web-scraper",
	"description": "Simple nodejs web scraper using cheerio",
	"version": "0.0.0",
	"scripts": {
		"dev": "concurrently \"vite\" \"nodemon scraper.js\"",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"prod": "vite build && pm2 start pm2.config.js --watch"
	},
	"dependencies": {
		"@fastify/cors": "^9.0.1",
		"cheerio": "^1.0.0-rc.12",
		"fastify": "^4.27.0",
		"nodemon": "^3.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"pm2": "^5.3.1",
		"serve": "^14.2.3",
		"typescript": "^5.2.2",
		"vite": "^5.2.0"
	}
}
